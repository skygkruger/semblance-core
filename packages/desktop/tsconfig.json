{
  "compilerOptions": {
    "strict": true,
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "moduleSuffixes": [".web", ""],
    "noUncheckedIndexedAccess": true,
    "paths": {
      "@semblance/core": ["../core/index.ts"],
      "@semblance/core/*": ["../core/*"],
      "@semblance/ui": ["../semblance-ui/index.ts"],
      "@semblance/ui/*": ["../semblance-ui/*"]
    },
    "baseUrl": "."
  },
  "include": ["src", "../semblance-ui/**/*.d.ts"]
}
